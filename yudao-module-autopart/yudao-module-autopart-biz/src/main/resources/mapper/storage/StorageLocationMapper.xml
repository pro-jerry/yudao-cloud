<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.iocoder.yudao.module.autopart.dal.mysql.storage.StorageLocationMapper">

    <select id="getCountByStorageId" resultType="java.lang.Long">
        select count(*) from autopart_storage_location
        where
            storage_id = #{storageId,jdbcType=BIGINT} and deleted = 0
    </select>
</mapper>